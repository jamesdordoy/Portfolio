{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "fruitcake/laravel-cors": "^v3.0.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "inertiajs/inertia-laravel": "^v0.6.9",
        "laravel/framework": "^v9.48.0",
        "laravel/horizon": "^v5.13.0",
        "laravel/jetstream": "^v2.15.0",
        "laravel/sanctum": "^v3.2.1",
        "laravel/telescope": "^v4.12.0",
        "laravel/tinker": "^v2.8.0",
        "protonemedia/inertiajs-tables-laravel-query-builder": "^1.4.0",
        "spatie/laravel-data": "^2.2.2",
        "spatie/laravel-query-builder": "^5.1.1",
        "spatie/laravel-typescript-transformer": "^2.1.6",
        "tightenco/ziggy": "^v1.5.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.8",
        "fakerphp/faker": "^v1.21.0",
        "laravel/pint": "^v1.4.0",
        "laravel/sail": "^v1.18.1",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^v6.4.0",
        "pestphp/pest": "^v1.22.3",
        "pestphp/pest-plugin-laravel": "^v1.4.0",
        "pestphp/pest-plugin-parallel": "^1.2",
        "phpunit/phpunit": "^9.5.28",
        "spatie/laravel-ignition": "^1.6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
